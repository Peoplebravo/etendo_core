<!--AB43CE049A494F1F9F2FE083D9C0D440-->  <HQLWHERECLAUSE><![CDATA[e.receipt = (case when @received_in@ = 'true' then 'Y' else 'N' end)
  and true = (case
     when (@date_from@ is null or @date_from@ = '' or @date_from@ = 'null') then true 
    when (@date_from@ is not null and @date_from@ <> '' and @date_from@ <> 'null' and e.paymentDate > TO_DATE(@date_from@, 'yyyy-MM-dd')) then true
   else false end)
  and true = (case when (@date_to@ is null or @date_to@ = '' or @date_to@ = 'null') then true
  when (@date_to@ is not null and @date_to@ <> '' and @date_to@ <> 'null' and e.paymentDate < TO_DATE(@date_to@, 'yyyy-MM-dd')) then true 
  else false end)
and e.status = 'RPAE'
and e.paymentMethod.id = @payment_method@
and e.account.id = @financial_account@
and e.organization.id in (select ot.parentOrganization.id from OrganizationTree as ot where ot.organization.id = @_org@)]]></HQLWHERECLAUSE>
